[flake8]
max-line-length = 100
ignore = E203,W503,E501
extend-ignore = E203,W503,E501
exclude =
    .git,
    __pycache__,
    build,
    dist,
    *.pyc,
    *.pyo,
    *.pyd,
    .Python,
    env,
    venv,
    .venv,
    ENV,
    env.bak,
    venv.bak,
    .poetry,
    migrations,
per-file-ignores =
    __init__.py:F401
    tests/*:S101
max-complexity = 10
